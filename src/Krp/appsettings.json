{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Krp": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Yarp.ReverseProxy.Forwarder.HttpForwarder": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss.fff "
      }
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointsDefaults": {
      //"Protocols": "Http2"
      "Protocols": "Http1AndHttp2"
    },
    "Endpoints": {
      "Http": {
        "Url": "http://+:80"
      }
    }
  }
}
