{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Krp": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Yarp.ReverseProxy.Forwarder.HttpForwarder": "Warning"
    },
    "Console": {
      "FormatterName": "simple",
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss.fff "
      }
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:81",
        "Protocols": "Http1"
      },
      "Http2": {
        "Url": "http://*:82",
        "Protocols": "Http2"
      },
      "Https": {
        "Url": "https://*:443",
        "Protocols": "Http1"
      }
    }
  }
}
